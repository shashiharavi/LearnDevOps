pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                echo 'Checking out from SCM'
	        checkout([$class: 'GitSCM', branches: [[name: '*/javasample']], 
		extensions: [], userRemoteConfigs: [[credentialsId: 'e55373a1-c730-430d-8e51-0601ab7b0007',
		url: 'https://github.com/shashiharavi/LearnDevOps.git']]])	
            }
        }
        stage('BUILD + UT') {
            steps {
                echo 'Building and Testing'
		sh 'mvn clean compile test package'
            }

        }
        
    }
    }
